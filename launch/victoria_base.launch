<launch>
    <!-- Launch rosserial to communicate with platform -->
    <!-- Parameters need to be configurable -->
    <node pkg="rosserial_python" type="serial_node.py" name="rosserial_node">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="57600"/>
    </node>

    <!-- Launch victoria_base to republish messages from platform -->
    <node pkg="victoria_base" type="victoria_base.py" name="victoria_base_node"/>
</launch>
